<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Automation" thread-count="5" parallel="tests">
<listeners>
<listener class-name="com.qa.Utility.Reporting"></listener>
</listeners> 
<test name="Flipkart Testing" thread-count="5" parallel="tests">
	<parameter name="Browser" value="chrome"/>
	
	<parameter name="Url" value="https://www.flipkart.com/"/>
<classes>
	<class name="com.qa.pages.Search"/>
	<class name="com.qa.Test_Scripts.Flipkart_pages"/> 
	<class name="com.qa.Test_Scripts.Flipkart_product_search"/> 
	<class name="com.qa.Test_Scripts.Brand_click"/> 

	
</classes>
</test>
</suite> 

 
